#--- this requires fasrcsw_defines.rpmmacros

#It's possible all the macros were definied with missing values (I don't know 
#how to properly "exit" a build outside of scriptlet sections).  This makes 
#sure we don't go any further in that case.
if [ -z "${FASRCSW_PROD:-}" ] || [ -z "${FASRCSW_DEV:-}" ]; then
	set +x
	echo "*** ERROR *** environment variables FASRCSW_PROD and/or FASRCSW_DEV undefined" >&2
	set -x
	exit 1
fi
